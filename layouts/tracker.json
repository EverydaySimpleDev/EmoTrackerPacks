  "tracker_default": {
    "type": "container",
    "background": "#000000",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "v_alignment": "stretch",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "content": {
                "type": "layout",
                "key": "shared_item_grid"
              }
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "vertical",
                "compact": true
              }
            }
          ]
        },
        {
          "type": "tabbed",
          "tabs": [
            {
              "title": "Zoom",
              "content": {
                "type": "tabbed",
                "tabs": [
                  {
                    "title": "Living Room",
                    "content": {
                      "type": "scroll",
                      "content": [
                        {
                          "type": "map",
                          "maps": [
                            "livingroommap"
                          ],
                          "min_height": 832
                        }
                      ]
                    }
                  }
                
                ]
              }
            }
          ]
        }
      ]
    }
  },
